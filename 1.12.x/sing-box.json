      "type": "selector",
      "outbounds": ["direct", "reject", "🇭🇰 自动选择", "🇭🇰 手动选择", "🇸🇬 自动选择", "🇸🇬 手动选择", "🇺🇸 自动选择", "🇺🇸 手动选择"],
      "default": "🇺🇸 自动选择"
