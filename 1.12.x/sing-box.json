{
  "dns": {
    "servers": [
      {
        "type": "udp",
        "tag": "mosdns",
        "server": "182.92.122.107",
        "server_port": 1053
      },
      {
        "type": "fakeip",
        "tag": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      { "clash_mode": "Direct", "server": "mosdns" },
      { "clash_mode": "Global", "server": "fakeip" },
      { "query_type": "HTTPS",  "action": "reject" },
      { "query_type": ["A", "AAAA"], "server": "fakeip", "rewrite_ttl": 0 }
    ],
    "final": "mosdns",
    "independent_cache": true
  },

  "route": {
    "rules": [
      { "action": "sniff", "sniffer": [ "http", "tls", "quic", "dns" ], "timeout": "500ms" },
      { "type": "logical", "mode": "or", "rules": [ { "port": 53  }, { "protocol": "dns" } ], "action": "hijack-dns" },
      { "ip_is_private": true, "outbound": "direct" },
      { "type": "logical", "mode": "or", "rules": [ { "port": 853 }, { "network": "udp", "port": 443 }, { "protocol": "stun" } ], "action": "reject" },
      { "clash_mode": "Global",              "outbound": "GLOBAL" },
      { "clash_mode": "Direct",              "outbound": "direct" },
      { "rule_set": "games@cn_domain",       "outbound": "direct" },
      { "rule_set": "googlefcm_domain",      "outbound": "direct" },
      { "rule_set": "microsoft@cn_domain",   "outbound": "direct" },
      { "rule_set": "category-ai-!cn_domain","outbound": "🇺🇸 美国" },
      { "rule_set": "youtube_domain",        "outbound": "🇺🇸 美国" },
      { "rule_set": "tiktok_domain",         "outbound": "🇺🇸 美国" },
      { "rule_set": "github_domain",         "outbound": "🇸🇬 狮城" },
      { "rule_set": "onedrive_domain",       "outbound": "☁️ 微软云盘" },
      { "domain_suffix":"googleapis.cn",     "outbound": "🔗 默认代理" },
      { "action": "resolve" },
      { "rule_set": [ "telegram_ip", "telegram_domain" ], "outbound": "🇸🇬 狮城" },
      { "rule_set": [ "cn_ip", "cn_domain" ], "outbound": "direct" },
      { "network":  [ "icmp" ],               "outbound": "direct" }
    ],
    "rule_set": [
      { "type": "remote", "tag": "cn_domain",             "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs" },
      { "type": "remote", "tag": "games@cn_domain",       "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-games@cn.srs" },
      { "type": "remote", "tag": "googlefcm_domain",      "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/googlefcm.srs" },
      { "type": "remote", "tag": "microsoft@cn_domain",   "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/microsoft@cn.srs" },
      { "type": "remote", "tag": "category-ai-!cn_domain","download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-ai-!cn.srs" },
      { "type": "remote", "tag": "youtube_domain",        "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs" },
      { "type": "remote", "tag": "tiktok_domain",         "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/tiktok.srs" },
      { "type": "remote", "tag": "github_domain",         "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/github.srs" },
      { "type": "remote", "tag": "onedrive_domain",       "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/onedrive.srs" },
      { "type": "remote", "tag": "telegram_domain",       "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/telegram.srs" },
      { "type": "remote", "tag": "telegram_ip",           "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/telegram.srs" },
      { "type": "remote", "tag": "cn_ip",                 "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs" }
    ],
    "final": "🔗 默认代理",
    "default_domain_resolver": "mosdns",
    "auto_detect_interface": true
  },
  "outbounds": [
    {
      "tag": "direct",
      "type": "direct"
    },
    { "tag": "✈️ 节点总览", "type": "selector", "outbounds": [] },
    { "tag": "🔗 默认代理", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 狮城", "🇺🇸 美国" ], "default": "🇭🇰 香港" },
    { "tag": "☁️ 微软云盘", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 狮城", "🇺🇸 美国" ], "default": "direct" },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 狮城", "🇺🇸 美国" ], "default": "🇭🇰 香港"
    },
    {
      "tag": "🇭🇰 香港", "type": "urltest", "outbounds": []
    },
    {
      "tag": "🇸🇬 狮城", "type": "urltest", "outbounds": []
    },
    {
      "tag": "🇺🇸 美国", "type": "urltest", "outbounds": []
    }
  ],

  "log": {
    "disabled": false,
    "level": "error",
    "output": "box.log",
    "timestamp": true
  },

  "inbounds": [
    {
      "type": "mixed", "listen": "0.0.0.0", "listen_port": 7890
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "singbox",
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true,
      "strict_route": true,
      "platform": { "http_proxy": { "enabled": true, "server": "127.0.0.1", "server_port": 7890 } }
    }
  ],

  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://ghfast.top/https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "direct"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    }
  }
}
