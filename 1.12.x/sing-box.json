{
  "dns": {
    "servers": [
      { "tag": "mosdns", "type": "udp", "server": "182.92.122.107", "server_port": 1053 },
      { "tag": "fakeip", "type": "fakeip","inet4_range": "198.18.0.0/15", "inet6_range": "fc00::/18" }
    ],
    "rules": [
      { "query_type": "HTTPS",  "action": "reject" },
      { "query_type": ["A", "AAAA"], "server": "fakeip", "rewrite_ttl": 1 }
    ],
    "final": "mosdns",
    "independent_cache": true
  },

  "route": {
    "rules": [
      { "action": "sniff", "sniffer": [ "http", "tls", "quic", "dns" ], "timeout": "500ms" },
      { "type": "logical", "mode": "or", "rules": [ { "port": 53  }, { "protocol": "dns" } ], "action": "hijack-dns" },
      { "ip_is_private": true, "outbound": "direct" },
      { "type": "logical", "mode": "or", "rules": [ { "port": 853 }, { "network": "udp", "port": 443 }, { "protocol": "stun" } ], "action": "reject" },
      { "clash_mode": "Global",              "outbound": "GLOBAL" },
      { "clash_mode": "Direct",              "outbound": "direct" },
      { "rule_set": "games@cn_domain",       "outbound": "direct" },
      { "rule_set": "googlefcm_domain",      "outbound": "direct" },
      { "rule_set": "category-ai-!cn_domain","outbound": "💬 智能助理" },
      { "rule_set": "youtube_domain",        "outbound": "📹 油管视频" },
      { "rule_set": "tiktok_domain",         "outbound": "🎵 海外抖音" },
      { "rule_set": "bing_domain",           "outbound": "🔗 默认代理" },
      { "rule_set": "github_domain",         "outbound": "📦 代码托管" },
      { "rule_set": "microsoft_domain",      "outbound": "direct" },
      { "action": "resolve" },
      { "rule_set": [ "telegram_ip", "telegram_domain" ], "outbound": "📲 电报消息" },
      { "rule_set": [ "cn_ip", "cn_domain" ], "outbound": "direct" },
      { "network":  [ "icmp" ], "outbound": "direct" }
    ],
    "rule_set": [
      { "tag": "cn_domain",               "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs" },
      { "tag": "games@cn_domain",         "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-games@cn.srs" },
      { "tag": "googlefcm_domain",        "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/googlefcm.srs" },
      { "tag": "microsoft_domain",        "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/microsoft.srs" },
      { "tag": "category-ai-!cn_domain",  "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-ai-!cn.srs" },
      { "tag": "youtube_domain",          "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs" },
      { "tag": "tiktok_domain",           "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/tiktok.srs" },
      { "tag": "bing_domain",             "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/bing.srs" },
      { "tag": "github_domain",           "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/github.srs" },
      { "tag": "telegram_domain",         "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/telegram.srs" },
      { "tag": "telegram_ip",             "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/telegram.srs" },
      { "tag": "cn_ip",                   "type": "remote", "download_detour": "direct", "url": "https://ghfast.top/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs" }
    ],
    "final": "🔗 默认代理",
    "default_domain_resolver": "mosdns",
    "auto_detect_interface": true
  },
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    { "tag": "✈️ 节点总览", "type": "selector", "outbounds": [] },
    { "tag": "🔗 默认代理", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 新加坡", "🇺🇸 美国" ], "default": "🇭🇰 香港" },
    { "tag": "📲 电报消息", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 新加坡", "🇺🇸 美国" ], "default": "🇸🇬 新加坡" },
    { "tag": "💬 智能助理", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 新加坡", "🇺🇸 美国" ], "default": "🇺🇸 美国" },
    { "tag": "📹 油管视频", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 新加坡", "🇺🇸 美国" ], "default": "🇺🇸 美国" },
    { "tag": "🎵 海外抖音", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 新加坡", "🇺🇸 美国" ], "default": "🇺🇸 美国" },
    { "tag": "📦 代码托管", "type": "selector", "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 新加坡", "🇺🇸 美国" ], "default": "🇸🇬 新加坡" },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [ "direct", "🇭🇰 香港", "🇸🇬 新加坡", "🇺🇸 美国" ], "default": "🇭🇰 香港"
    },
    {
      "tag": "🇭🇰 香港", "type": "urltest", "outbounds": []
    },
    {
      "tag": "🇸🇬 新加坡", "type": "urltest", "outbounds": []
    },
    {
      "tag": "🇺🇸 美国", "type": "urltest", "outbounds": []
    }
  ],

  "log": {
    "disabled": false,
    "level": "error",
    "output": "box.log",
    "timestamp": true
  },

  "inbounds": [
    {
      "type": "mixed", "listen": "0.0.0.0", "listen_port": 7890
    },
    {
      "type": "tun",
      "interface_name": "singbox",
      "address": [
        "172.19.0.0/30"
      ],
      "auto_route": true,
      "strict_route": true,
      "route_exclude_address": [ "192.168.0.0/16", "fc00::/7" ],
      "platform": { "http_proxy": { "enabled": true, "server": "127.0.0.1", "server_port": 7890 } }
    }
  ],

  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://ghfast.top/https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "direct"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    }
  }
}
