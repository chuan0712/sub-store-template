{
  "log": {
    "disabled": false,
    "level": "error",
    "timestamp": true
  },


  "dns": {
    "servers": [
      { "tag": "mosdns", "type": "udp", "server": "182.92.122.107", "server_port": 1053 },
      { "tag": "fakeip", "type": "fakeip","inet4_range": "198.18.0.0/15", "inet6_range": "fc00::/18" }
    ],
    "rules": [
      { "clash_mode": "Direct", "action": "route", "server": "mosdns" },
      { "clash_mode": "Global", "action": "route", "server": "fakeip" },
      { "query_type": "HTTPS",  "action": "reject" },
      { "query_type": ["A", "AAAA"], "action": "route", "server": "fakeip", "rewrite_ttl": 1 }
    ],
    "final": "mosdns",
    "independent_cache": true
  },

  "route": {
    "default_domain_resolver": "mosdns",
    "rules": [
      { "action": "sniff" },
      { "type": "logical", "mode": "or", "rules": [ { "port": 53  }, { "protocol": "dns" } ], "action": "hijack-dns" },
      { "ip_is_private": true, "outbound": "direct" },
      { "type": "logical", "mode": "or", "rules": [ { "port": 853 }, { "network": "udp", "port": 443 }, { "protocol": "stun" } ], "action": "reject" },

      { "clash_mode": "Global",              "outbound": "GLOBAL" },
      { "clash_mode": "Direct",              "outbound": "DIRECT" },
      { "rule_set": "steam@cn_domain",       "outbound": "direct" },
      { "rule_set": "googlefcm_domain",      "outbound": "direct" },
      { "rule_set": "microsoft@cn_domain",   "outbound": "direct" },
      { "rule_set": "category-ai-cn_domain", "outbound": "direct" },
      { "rule_set": "category-ai-!cn_domain","outbound": "💬 智能助理" },
      { "rule_set": "google-play_domain",    "outbound": "🔗 默认代理" },
      { "rule_set": "youtube_domain",        "outbound": "📹 油管视频" },
      { "action": "resolve" },
      { "rule_set": [ "telegram_ip", "telegram_domain" ],"outbound": "📲 电报消息" },
      { "rule_set": [ "cn_ip", "cn_domain" ],            "outbound": "direct" }
    ],

    "rule_set": [
      {
        "tag": "cn_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs"
      },
      {
        "tag": "steam@cn_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/steam@cn.srs"
      },
      {
        "tag": "googlefcm_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/googlefcm.srs"
      },
      {
        "tag": "microsoft@cn_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/microsoft@cn.srs"
      },
      {
        "tag": "category-ai-cn_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-ai-cn.srs"
      },
      {
        "tag": "category-ai-!cn_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-ai-!cn.srs"
      },
      {
        "tag": "youtube_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs"
      },
      {
        "tag": "telegram_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/telegram.srs"
      },
      {
        "tag": "google-play_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google.srs"
      },
      {
        "tag": "cn_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs"
      },
      {
        "tag": "telegram_ip",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/telegram.srs"
      }
    ],
    "final": "🔗 默认代理",
    "auto_detect_interface": true
},


  "outbounds": [
    {
      "tag": "🔗 默认代理",
      "type": "selector",
      "outbounds": ["direct", "reject", "🇭🇰 自动选择", "🇭🇰 手动选择", "🇸🇬 自动选择", "🇸🇬 手动选择", "🇺🇸 自动选择", "🇺🇸 手动选择"],
